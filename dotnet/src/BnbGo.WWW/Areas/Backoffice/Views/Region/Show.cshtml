@model ICollection<BnbGo.Models.Region>
@{
    ViewBag.Title = "Region detail page";
    ViewBag.SubTitle = "All information about this specific region";
    ViewBag.Description = "View this region";
    Layout = "~/Areas/Backoffice/Views/Shared/_Layout.cshtml";
}
<div id="submenu">
    <div class="col col-xl-3 col-la-3 col-md-6 col-sm-6 col-xs-6">
        <a class="button" asp-controller="Region" asp-action="Index">Overview</a>
    </div>
    @foreach (var region in Model)
    {
    <div class="col col-xl-3 col-la-3 col-md-6 col-sm-6 col-xs-6">
        <a class="button" asp-controller="Region" asp-action="Edit" asp-route-id="@region.Id">Edit</a>
    </div>
    }
</div>

@Html.Partial("_DetailPartial", @Model)