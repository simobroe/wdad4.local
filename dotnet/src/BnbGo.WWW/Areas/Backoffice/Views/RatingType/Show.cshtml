@model ICollection<BnbGo.Models.RatingType>
@{
    ViewBag.Title = "Rating type detail page";
    ViewBag.SubTitle = "All information about this specific rating type";
    ViewBag.Description = "View this renttype";
    Layout = "~/Areas/Backoffice/Views/Shared/_Layout.cshtml";
}
<div id="submenu">
    <div class="col col-xl-3 col-la-3 col-md-6 col-sm-6 col-xs-6">
        <a class="button" asp-controller="RatingType" asp-action="Index">Overview</a>
    </div>
    @foreach (var ratingtype in Model)
    {
    <div class="col col-xl-3 col-la-3 col-md-6 col-sm-6 col-xs-6">
        <a class="button" asp-controller="RatingType" asp-action="Edit" asp-route-id="@ratingtype.Id">Edit</a>
    </div>
    }
</div>

@Html.Partial("_DetailPartial", @Model)