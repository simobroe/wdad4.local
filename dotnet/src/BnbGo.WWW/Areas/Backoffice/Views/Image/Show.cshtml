@model ICollection<BnbGo.Models.Image>
@{
    ViewBag.Title = "Image detail page";
    ViewBag.SubTitle = "All information about this specific image";
    Layout = "~/Areas/Backoffice/Views/Shared/_Layout.cshtml";
}
<div id="submenu">
    <div class="col col-xl-3 col-la-3 col-md-6 col-sm-6 col-xs-6">
        <a class="button" asp-controller="Image" asp-action="Index">Overview</a>
    </div>
    @foreach (var image in Model)
    {
    <div class="col col-xl-3 col-la-3 col-md-6 col-sm-6 col-xs-6">
        <a class="button" asp-controller="Image" asp-action="Edit" asp-route-id="@image.Id">Edit</a>
    </div>
    }
</div>

@Html.Partial("_DetailPartial", @Model)