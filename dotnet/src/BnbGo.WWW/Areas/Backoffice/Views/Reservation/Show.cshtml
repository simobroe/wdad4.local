@model ICollection<BnbGo.Models.Reservation>
@{
    ViewBag.Title = "Reservation detail page";
    ViewBag.SubTitle = "All information about this specific reservation";
    ViewBag.Description = "View this reservation";
    Layout = "~/Areas/Backoffice/Views/Shared/_Layout.cshtml";
}
<div id="submenu">
    <div class="col col-xl-3 col-la-3 col-md-6 col-sm-6 col-xs-6">
        <a class="button" asp-controller="Reservation" asp-action="Index">Overview</a>
    </div>
    @foreach (var reservation in Model)
    {
    <div class="col col-xl-3 col-la-3 col-md-6 col-sm-6 col-xs-6">
        <a class="button" asp-controller="Reservation" asp-action="Edit" asp-route-id="@reservation.Id">Edit</a>
    </div>
    }
</div>

@Html.Partial("_DetailPartial", @Model)