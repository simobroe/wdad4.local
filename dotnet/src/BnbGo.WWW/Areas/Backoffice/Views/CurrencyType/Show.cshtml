@model ICollection<BnbGo.Models.CurrencyType>
@{
    ViewBag.Title = "Currency type detail page";
    ViewBag.SubTitle = "All information about this specific currency type";
    Layout = "~/Areas/Backoffice/Views/Shared/_Layout.cshtml";
}
<div id="submenu">
    <div class="col col-xl-3 col-la-3 col-md-6 col-sm-6 col-xs-6">
        <a class="button" asp-controller="CurrencyType" asp-action="Index">Overview</a>
    </div>
    @foreach (var currencytype in Model)
    {
    <div class="col col-xl-3 col-la-3 col-md-6 col-sm-6 col-xs-6">
        <a class="button" asp-controller="CurrencyType" asp-action="Edit" asp-route-id="@currencytype.Id">Edit</a>
    </div>
    }
</div>

@Html.Partial("_DetailPartial", @Model)