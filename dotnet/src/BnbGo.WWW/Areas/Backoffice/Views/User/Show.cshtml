@model ICollection<BnbGo.Models.Security.ApplicationUser>
@{
    ViewBag.Title = "User detail page";
    ViewBag.SubTitle = "All information about this specific user";
    ViewBag.Description = "View this user";
    Layout = "~/Areas/Backoffice/Views/Shared/_Layout.cshtml";
}
<div id="submenu">
    <div class="col col-xl-3 col-la-3 col-md-6 col-sm-6 col-xs-6">
        <a class="button" asp-controller="User" asp-action="Index">Overview</a>
    </div>
    @foreach (var user in Model)
    {
    <div class="col col-xl-3 col-la-3 col-md-6 col-sm-6 col-xs-6">
        <a class="button" asp-controller="User" asp-action="Edit" asp-route-id="@user.Id">Edit</a>
    </div>
    }
</div>

@Html.Partial("_DetailPartial", @Model)