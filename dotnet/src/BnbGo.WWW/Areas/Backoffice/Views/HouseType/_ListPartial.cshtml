@model ICollection<BnbGo.Models.HouseType>
@Html.AntiForgeryToken()
@if (Model != null && Model.Count() > 0)
{
      <table>
        <thead>
          <tr>
            <th>Name</th>
            <th>Description</th>
          </tr>
        </thead>
        <tbody>
          @foreach (var housetype in Model)
          {
            <tr>
              <td>@housetype.Name</td>
              <td>@housetype.Description</td>
            </tr>
          }
        </tbody>
      </table>
}
else
{
    <p>No house types in the database!</p>
}
</div>